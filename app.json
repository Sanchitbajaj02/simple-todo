{
  "expo": {
    "name": "simple-todo",
    "slug": "simple-todo",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sanchitbajaj02.todoapp",
      "buildNumber": "0.1.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sanchitbajaj02.todoapp",
      "versionCode": 1
    },
    "web": {
      "icon": "./assets/icon.png"
    },
    "description": "Simple Todo Application made by React Native"
  }
}